		"@types/node": "^18.19.124",
		"@types/semver": "^7.7.1",
		"@typescript-eslint/eslint-plugin": "^8.43.0",
		"@typescript-eslint/parser": "^8.43.0",
		"clipanion": "^4.0.0-rc.4",
		"consola": "^3.4.2",
		"convert": "^5.12.0",
		"cross-env": "^10.0.0",
		"eslint": "^9.35.0",
		"json5": "^2.2.3",
		"release-it": "^19.0.4",
		"semver": "^7.7.2",
		"ts-node": "^10.9.2",
		"tslib": "^2.8.1",
		"tsup": "^8.5.0",
		"typescript": "^5.9.2"
	},
	"packageManager": "yarn@4.9.4"
}

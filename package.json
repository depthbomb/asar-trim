{
	"version": "0.3.0",
	"name": "asar-trim",
	"author": "depthbomb",
	"description": "An experimental CLI tool that trims unnecessary files from asar archives for Electron applications.",
	"license": "MIT",
	"main": "dist/cli.js",
	"bin": "dist/cli.js",
	"publishConfig": {
		"access": "public"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/depthbomb/asar-trim.git"
	},
	"bugs": {
		"url": "https://github.com/depthbomb/asar-trim/issues"
	},
	"files": [
		"dist"
	],
	"scripts": {
		"build": "cross-env NODE_ENV=production webpack --config webpack.config.ts && yarn postbuild",
		"postbuild": "ts-node scripts/post-build",
		"prerelease": "yarn build",
		"release": "release-it",
		"lint": "eslint ./src --ext .ts"
	},
	"dependencies": {
		"@octokit/request": "^5.6.3",
		"asar": "^3.1.0",
		"clipanion": "^3.2.0-rc.10",
		"json5": "^2.2.1",
		"pretty-bytes": "^6.0.0",
		"semver": "^7.3.7",
		"tasuku": "^1.0.2"
	},
	"devDependencies": {
		"@babel/core": "^7.17.9",
		"@babel/plugin-transform-runtime": "^7.17.0",
		"@babel/preset-env": "^7.16.11",
		"@babel/preset-typescript": "^7.16.7",
		"@tsconfig/recommended": "^1.0.1",
		"@types/babel__core": "^7.1.19",
		"@types/babel__plugin-transform-runtime": "^7.9.2",
		"@types/babel__preset-env": "^7.9.2",
		"@types/cli-progress": "^3.9.2",
		"@types/eslint": "^8.4.1",
		"@types/node": "^17.0.29",
		"@types/semver": "^7",
		"@types/webpack-node-externals": "^2.5.3",
		"@typescript-eslint/eslint-plugin": "^5.21.0",
		"@typescript-eslint/parser": "^5.21.0",
		"babel-loader": "^8.2.5",
		"cross-env": "^7.0.3",
		"eslint": "^8.14.0",
		"release-it": "^14.14.2",
		"terser-webpack-plugin": "^5.3.1",
		"ts-node": "^10.7.0",
		"tslib": "^2.4.0",
		"typescript": "^4.6.3",
		"webpack": "^5.72.0",
		"webpack-cli": "^4.9.2",
		"webpack-node-externals": "^3.0.0"
	},
	"packageManager": "yarn@3.2.0"
}

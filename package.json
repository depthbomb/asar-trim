{
	"version": "1.0.0",
	"name": "asar-trim",
	"author": "depthbomb",
	"description": "An experimental CLI tool that trims unnecessary files from asar archives for Electron applications.",
	"license": "MIT",
	"main": "dist/cli.js",
	"bin": "dist/cli.js",
	"publishConfig": {
		"access": "public"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/depthbomb/asar-trim.git"
	},
	"bugs": {
		"url": "https://github.com/depthbomb/asar-trim/issues"
	},
	"files": [
		"dist"
	],
	"scripts": {
		"start": "node .",
		"dist": "cross-env NODE_ENV=production tsup",
		"build": "cross-env NODE_ENV=development tsup",
		"release": "yarn dist && release-it"
	},
	"dependencies": {
		"asar": "^3.1.0",
		"clipanion": "^3.2.0-rc.11",
		"consola": "^2.15.3",
		"convert": "^4.5.0",
		"json5": "^2.2.1",
		"semver": "^7.3.7"
	},
	"devDependencies": {
		"@swc/core": "^1.2.215",
		"@tsconfig/recommended": "^1.0.1",
		"@types/eslint": "^8.4.5",
		"@types/node": "^18.0.5",
		"@types/semver": "^7.3.10",
		"@typescript-eslint/eslint-plugin": "^5.30.6",
		"@typescript-eslint/parser": "^5.30.6",
		"cross-env": "^7.0.3",
		"eslint": "^8.20.0",
		"release-it": "^15.1.2",
		"ts-node": "^10.9.1",
		"tslib": "^2.4.0",
		"tsup": "^6.1.3",
		"typescript": "^4.7.4"
	},
	"packageManager": "yarn@3.2.1"
}
